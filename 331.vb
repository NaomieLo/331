Sub CreateQuestionButtons()
    Dim ws As Worksheet
    Dim i As Integer

    ' Set the worksheet
    Set ws = ThisWorkbook.Sheets("Dashboard")

    ' Loop to create buttons and assign macros
    For i = 19 To 27
        With ws.Buttons.Add(50, 20 * i, 80, 20)
            .Caption = "Question " & i
            
        End With
    Next i
End Sub

Sub ToggleQuestion(ByVal questionNum As Integer)
    Dim ws As Worksheet
    Dim hideStartRow As Integer

    ' Set the worksheet
    Set ws = ThisWorkbook.Sheets("Questions")

    ' Determine the starting row to hide
    hideStartRow = 1 + questionNum

    ' Toggle visibility of rows
    With ws.Rows(hideStartRow & ":100")
        .EntireRow.Hidden = Not .EntireRow.Hidden
    End With
End Sub


